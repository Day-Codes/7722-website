<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>FRC Team Events</title>
    <link rel="stylesheet" href="styles.css">
</head>
<body>
    <div class="container">
        <h1>FRC Team Events</h1>
        <table class="events-table">
            <thead>
                <tr>
                    <th>Location</th>
                    <th>Event Name</th>
                    <th>Time</th>
                </tr>
            </thead>
            <tbody>
                <% events.forEach(event => { %>
                    <tr>
                        <td><%= event.location %></td>
                        <td><%= event.eventName %></td>
                        <td><%= event.time %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
</body>
</html>
